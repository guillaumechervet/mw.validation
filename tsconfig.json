{
  "compileOnSave": false,
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "jsx": "react",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "target": "es5",
    "lib": ["es2016", "dom"]
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "build", "lib", "bower_components", "wwwroot"]
}
